


<div class="row">
<%= form_for(@venue) do |f| %>
<% if @venue.errors.any? %>
<div id="error_explanation">
  <h2><%= pluralize(@venue.errors.count, "error") %> prohibited this venue from being saved:</h2>

  <ul>
    <% @venue.errors.full_messages.each do |message| %>
    <li><%= message %></li>
    <% end %>
  </ul>
</div>
<% end %>
<div class="new_venue_input">
  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  
  <div class="field">
    <%= f.label :logo %><br />
    <%= f.file_field :logo %>
  </div>


<div class="field">
  <%= f.label :description %><br>
  <%= f.text_area :description %>
</div>
<div class="field">
  <%= f.label :location_id %><br>
  <%= f.collection_select(:location_id, Location.all, :id, :name) %>
</div>
</div>
<div class="actions">
  <%= f.submit %>
</div>
<% end %>


